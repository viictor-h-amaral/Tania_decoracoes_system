<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="FontAwesomeSolid">
        <Setter Property="TextElement.FontFamily" Value="pack://application:,,,/Fonts/FontAwesome/#Font Awesome 6 Free Solid" />
    </Style>
    <Style x:Key="FontAwesomeRegular">
        <Setter Property="TextElement.FontFamily" Value="pack://application:,,,/Fonts/FontAwesome/#Font Awesome 6 Free Regular" />
    </Style>
    <Style x:Key="FontAwesomeRegularBrands">
        <Setter Property="TextElement.FontFamily" Value="pack://application:,,,/Fonts/FontAwesome/#Font Awesome 6 Brands Regular" />
    </Style>
</ResourceDictionary>