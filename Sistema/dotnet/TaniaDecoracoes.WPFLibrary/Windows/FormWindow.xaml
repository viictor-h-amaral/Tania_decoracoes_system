<Window x:Class="TaniaDecoracoes.WPFLibrary.Windows.FormWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaniaDecoracoes.WPFLibrary.Windows"
        xmlns:userControls="clr-namespace:TaniaDecoracoes.WPFLibrary.UserControls"
        mc:Ignorable="d"
        Title="FormWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Button Content="btnClose" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding CloseCommand}"/>
        <userControls:CommonForm x:Name="Form" DataContext="{Binding FormVM}"/>
    </StackPanel>
</Window>
